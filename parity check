byte= ["1001100", "1101010", "1011101"]

def paritycheck(byte):
    count=0
    for i in byte:
        for a in i:
            if (a) == "1":
                count+=1
            else:
                continue
        if count%2==0:
            print("the parity bit is 0, for", i, "=",i+"0")
        else:
            print("the parity bit is 1 for", i, "=",i+"1")
        count = 0   

paritycheck(byte)

