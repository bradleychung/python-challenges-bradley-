import turtle
wn = turtle.Screen()
alex = turtle.Turtle()

pirate = [160, -43, 270, -97, -43, 200, -940, 17, -86]

for i in pirate:
     alex.forward(100)
     alex.right(i)
     
     
import turtle
wn = turtle.Screen()
alex = turtle.Turtle()
heading= 0 
pirate = [160, -43, 270, -97, -43, 200, -940, 17, -86]

for i in pirate:
     alex.forward(100)
     alex.right(i)
     totalheading=(heading+i)
     
print(totalheading%360)


import turtle
wn = turtle.Screen()
alex = turtle.Turtle()

alex.left(36)
for i in range(5):
    alex.forward(300)
    alex.left(144)


import turtle
wn = turtle.Screen()
alex = turtle.Turtle()
alex.stamp()
alex.penup()
for i in range(12):
    alex.forward(140)
    alex.pendown()
    alex.forward(30)
    alex.penup()
    alex.forward(30)
    alex.stamp()
    alex.left(180)
    alex.forward(200)
    alex.left(330)
