for i in range(1000):
    print("We like Python's turtles!")
    
    
for m in ["january", "february", "march", "april", "may", "june", "july", "august", "september", "october", "november", "december"]:
    print("One of the months of the year is ", m)
    
    
for xs in [12, 10, 32, 3, 66, 17, 42, 99, 20]:
    print(xs)
    
for xs in [12, 10, 32, 3, 66, 17, 42, 99, 20]:
    print(xs**2)
    
    
total = 0
for xs in [12, 10, 32, 3, 66, 17, 42, 99, 20]:
    total= total+xs    
print(total)


total = 1
for xs in [12, 10, 32, 3, 66, 17, 42, 99, 20]:
    total = (xs*total)
print(total)


import turtle
wn = turtle.Screen()
alex = turtle.Turtle()
 
for i in range(3):
     alex.forward(80)
     alex.left(120)
     
     
