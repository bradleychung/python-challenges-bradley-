import turtle
wn = turtle.Screen()

def square (t,sz):
    for i in range(4):
        turtle.forward(sz)
        turtle.left(90)
    turtle.penup()
    turtle.forward(sz*2)
    turtle.pendown()

bob = turtle.Turtle
for i in range(5):
    square(bob,20)
    
    
    
   
import turtle
bob = turtle.Turtle
wn = turtle.Screen()
turtle.speed(1)
sz = 20

def square (t,sz):
    for i in range(4):
        turtle.forward(sz)
        turtle.left(90)
    turtle.penup()
    for i in range(2):
        turtle.right(90)
        turtle.forward(10)
    turtle.right(90)
    turtle.pendown()

for i in range(5):
    square(bob,sz)
    sz = sz + 20
